{
    "site_name": "7h4Worker",
    "site_slogon": "技术探索 · 知识分享 · 持续学习",
    "site_root_url": "/lhfdoc.github.io/",
    "site_domain": "7h4worker.github.io",
    "site_protocol": "https",
    "config_template_dir": "config",
    "route": {
        "docs": {
            "/embedded_dev/": "docs/embedded_dev",
            "/bci_paradigms/": "docs/bci_paradigms",
            "/bci_hardware/": "docs/bci_hardware",
            "/bci_algorithms/": "docs/bci_algorithms",
            "/ui_development/": "docs/ui_development",
            "/edge_ai/": "docs/edge_ai",
            "/essays/": "docs/essays"
        },
        "pages": {
            "/": "pages/index"
        },
        "assets": {
            "/static/": "static"
        }
    },
    "plugins": {
        "teedoc-plugin-markdown-parser":{
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-theme-default":{
            "from": "pypi",
            "config": {
                "dark": true,
                "show_print_page": false,
                "env":{
                    "main_color": "#007ACC"
                }
            }
        },
        "teedoc-plugin-search":{
            "from": "pypi",
            "config": {
                "search_hint": "搜索文档",
                "input_hint": "输入关键词",
                "loading_hint": "正在搜索...",
                "download_err_hint": "下载失败，请刷新重试",
                "other_docs_result_hint": "来自其他文档的结果",
                "curr_doc_result_hint": "当前文档搜索结果"
            }
        },
        "teedoc-plugin-assets": {
            "from": "pypi",
            "config": {
                "header_items": [
                    "/static/css/custom.css"
                ],
                "footer_items": [
                    "/static/js/custom.js"
                ],
                "env":{
                    "main_color": "#007ACC",
                    "second_color": "#005a9e"
                }
            }
        }
    },
    "rebuild_changes_delay": 1,
    "robots":{
        "User-agent": "*"
    },
    "layout_root_dir": "layout"
}
